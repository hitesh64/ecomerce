{
    "version": 2,
    "builds": [
        {
            "src": "js/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/js/server.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}